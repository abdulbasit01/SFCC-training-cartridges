<?xml version="1.0"?>
<form xmlns="http://www.demandware.com/xml/form/2008-04-19" secure="false">
    <field formid="fname" parse-error="error.message.parse.fname.profile.form" label="label.input.firstname.profile" mandatory="true" max-length="50" type="string" />

    <field formid="lname" parse-error="error.message.parse.lname.profile.form" label="label.input.lastname.profile" mandatory="false" max-length="50" type="string" />

    <field formid="email" parse-error="error.message.parse.email.profile.form" label="label.input.email.profile" mandatory="true" max-length="50" regexp="^[\w.%+-]+@[\w.-]+\.[\w]{2,6}$" type="string" />

    <field formid="organization" label="label.input.organization.profile" type="string" mandatory="true">
        <option optionid="public" label="label.input.public.profile" value="label.input.public.profile"/>    
        <option optionid="private" label="label.input.private.profile" value="label.input.private.profile"/>    
    </field>

    <field formid="primaryintrest" label="label.input.pintrest.profile" type="string" mandatory="true">
        <option optionid="sports" label="label.input.sports.profile" value="label.input.sports.profile"/>    
        <option optionid="music" label="label.input.music.profile" value="label.input.music.profile"/>    
        <option optionid="movies" label="label.input.movies.profile" value="label.input.movies.profile"/>    
        <option optionid="networking" label="label.input.networking.profile" value="label.input.networking.profile"/>    
    </field>
    <field formid="marketingemial" label="label.input.marketingemial.profile" mandatory="false" type="boolean" />

</form>