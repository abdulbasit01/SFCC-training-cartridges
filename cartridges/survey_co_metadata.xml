<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <custom-type type-id="surveyForm">
        <display-name xml:lang="x-default">Survey Form</display-name>
        <description xml:lang="x-default">Custom object used to store survey information.</description>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>site</storage-scope>
        <key-definition attribute-id="email">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="firstName">
                <display-name xml:lang="x-default">First Name</display-name>
                <description xml:lang="x-default">Enter the first name for the subscription</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="lastName">
                <display-name xml:lang="x-default">Last Name</display-name>
                <description xml:lang="x-default">Last name for subscription.</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="organization">
                <display-name xml:lang="x-default">Organization</display-name>
                <description xml:lang="x-default">Describes the type of an organization</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="primaryIntrest">
                <display-name xml:lang="x-default">Primary Interest</display-name>
                <description xml:lang="x-default">Describes your interet</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="merketingEmail">
                <display-name xml:lang="x-default">Marketing Email</display-name>
                <description xml:lang="x-default">Do you want to recieve an email about marketing</description>
                <type>boolean</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="presentation">
                <display-name xml:lang="x-default">Presentation</display-name>
                <attribute attribute-id="email"/>
                <attribute attribute-id="firstName"/>
                <attribute attribute-id="lastName"/>
                <attribute attribute-id="organization"/>
                <attribute attribute-id="primaryIntrest"/>
                <attribute attribute-id="merketingEmail"/>
                <attribute attribute-id="creationDate" system="true"/>
            </attribute-group>
        </group-definitions>
    </custom-type>

</metadata>
